                    <Locked>False</Locked>
                    <Width>250</Width>
                    <Left>18</Left>
                    <Top>552</Top>
                    <ZOrder>3</ZOrder>
                    <ID>3</ID>
                    <Name>btnoutsidePherian1Next</Name>
                  </GUIButton>
                </Controls>
